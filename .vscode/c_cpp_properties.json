        "${workspaceFolder}/nowa",
        "${workspaceFolder}/../qmk_firmware/platforms",
        "${workspaceFolder}/../qmk_firmware/platforms/avr",
        "${workspaceFolder}/../qmk_firmware/platforms/avr/drivers",
        "${workspaceFolder}/../qmk_firmware/quantum",
        "${workspaceFolder}/../qmk_firmware/quantum/audio",
        "${workspaceFolder}/../qmk_firmware/quantum/bootmagic",
        "${workspaceFolder}/../qmk_firmware/quantum/logging",
        "${workspaceFolder}/../qmk_firmware/quantum/process_keycode",
        "${workspaceFolder}/../qmk_firmware/quantum/sequencer",
        "${workspaceFolder}/../qmk_firmware/tmk_core/protocol",
        "${workspaceFolder}/../qmk_firmware/tmk_core/protocol",
        "/usr/include",
        "/usr/lib/avr/include",
        "/usr/local/include"
